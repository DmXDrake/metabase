{
    "name": "laravolt/metabase",
    "description": "Embed Metabase dashboard and question in your application",
    "keywords": [
        "laravolt",
        "laravel",
        "metabase"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bayu Hendra Winata",
            "email": "uyab.exe@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "*",
        "lcobucci/jwt": "^5.0.0"
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\Metabase\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravolt\\Metabase\\MetabaseServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
